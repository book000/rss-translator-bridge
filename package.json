{
  "name": "rss-translator-bridge",
  "version": "1.0.0",
  "description": "RSS Feed translation bridge server",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "tsx ./src/index.ts",
    "dev": "tsx watch ./src/index.ts",
    "build": "tsc",
    "lint": "run-z lint:prettier,lint:eslint,lint:tsc",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint .",
    "lint:tsc": "tsc --noEmit",
    "fix": "run-z fix:prettier,fix:eslint",
    "fix:prettier": "prettier --write .",
    "fix:eslint": "eslint --fix .",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "rss",
    "translator",
    "bridge",
    "fastify"
  ],
  "author": "book000",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@fastify/static": "^8.2.0",
    "axios": "^1.6.0",
    "fastify": "^4.25.0",
    "rss-parser": "^3.13.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@book000/eslint-config": "^1.7.2",
    "@types/jest": "^29.5.0",
    "@types/node": "^22.0.0",
    "@types/xml2js": "^0.4.14",
    "eslint": "^9.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "run-z": "^2.1.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.0.0"
}
